{
  "name": "@faasjs/ant-design",
  "version": "0.0.1-development",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "start-storybook",
    "build": "build-storybook",
    "prepack": "rollup -c",
    "test": "jest --coverage --ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faasjs/ant-design.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/faasjs/ant-design/issues"
  },
  "homepage": "https://faasjs.com/ant-design",
  "funding": "https://github.com/sponsors/faasjs",
  "engines": {
    "npm": ">=8.0.0"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@faasjs/eslint-config-react": "^0.0.2-beta.302",
    "@storybook/addon-actions": "*",
    "@storybook/addon-essentials": "*",
    "@storybook/addon-links": "*",
    "@storybook/addon-postcss": "*",
    "@storybook/builder-webpack5": "*",
    "@storybook/manager-webpack5": "*",
    "@storybook/react": "*",
    "webpack": "^5",
    "rollup": "*",
    "rollup-plugin-typescript2": "*",
    "swc-loader": "*",
    "@swc/jest": "*",
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "jest": "*",
    "trim": ">=0.0.3",
    "immer": ">=9.0.6",
    "@types/lodash": "*"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "antd": "*",
    "@ant-design/icons": "*",
    "lodash": "*"
  },
  "eslintConfig": {
    "extends": [
      "@faasjs/react"
    ]
  },
  "jest": {
    "rootDir": "src",
    "transform": {
      ".(jsx|tsx?)": "@swc/jest"
    },
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "**/*.tsx"
    ],
    "testRegex": "/*\\.test\\.tsx?$",
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/style-mock.js"
    },
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ]
  }
}
